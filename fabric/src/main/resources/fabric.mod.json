{
  "schemaVersion": 1,
  "id": "unitemplate",
  "version": "${version}",
  "name": "UniTemplate",
  "description": "This is an example description! Tell everyone what your mod is about!",
  "authors": [
    "topi_banana"
  ],
  "contact": {
    "homepage": "https://topi-banana.com/",
    "sources": "https://github.com/topi-banana/unitemplate"
  },
  "license": "CC0-1.0",
  "icon": "assets/unitemplate/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "com.topi_banana.unitemplate.fabric.FabricMain"
    ],
    "client": [
      "com.topi_banana.unitemplate.fabric.FabricMain"
    ],
    "server": [
      "com.topi_banana.unitemplate.fabric.FabricMain"
    ]
  },
  "mixins": [
    "unitemplate.mixins.json",
    {
      "config": "unitemplate.client.mixins.json",
      "environment": "client"
    }
  ],
  "depends": {
    "fabricloader": ">=0.16.7",
    "minecraft": "~1.21.3",
    "java": ">=21",
    "fabric-api": "*"
  },
  "suggests": {
    "another-mod": "*"
  }
}